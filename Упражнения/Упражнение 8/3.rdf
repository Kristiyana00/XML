                    cd:company="http://www.myshop.bg/cd/PavarottiGalaConcert/DECCA"
                    cd:price="http://www.myshop.bg/cd/PavarottiGalaConcert/9.90"
                    cd:year="http://www.myshop.bg/cd/PavarottiGalaConcert/1991"
    />
    <!-- Създаваме елемента Description за Soulsville, който има атрибути artist, country, company, price, year.
